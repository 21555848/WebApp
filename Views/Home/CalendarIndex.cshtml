
@{
    ViewData["Title"] = "CalendarIndex";
}

<h1>CalendarIndex</h1>

<hr />

@if (ViewBag.EventList!=null)
{
    <table class="table table-hover">
        <tr> 
            <th> Event tittle</th>
            <th>Organizer </th>
            <th>Description </th>
            <th>StartTime</th>
            <th>End Time</th>
            
        </tr>
        @foreach (var item in ViewBag.EventList as List<WebApp.Controllers.calendarEvent>)
        {
            <tr>
                <td>@item.Summary</td>
                <td>@item.Organizer</td>
                <td>@item.Description</td>
                <td>@item.StartTime</td>
                <td>@item.EndTime</td>
            </tr>
        }
    </table>
}
else
{
    <div class="alert-danger"> No Upcoming event</div>
}
}
